datasource db {
  provider = "postgresql"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model users {
    id Int @id @default(autoincrement())
    phone_country_code String
    phone_national_number String @unique
    email_address String @unique
    first_name String
    last_name String
}
